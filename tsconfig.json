{
	"extends": "astro/tsconfigs/strict",
	"typescript.inlayHints.parameterNames.enabled": "all",
	"compilerOptions": {
		"target": "ES6",
		"strictNullChecks": true,
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"allowJs": true,
		"baseUrl": ".",
		"paths": {
			"@*": ["src/*"],
			"@styles/main": ["src/styles/_main.scss"]
		}
	}
}
